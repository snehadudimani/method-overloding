classDisplayOverloading
{
public void disp(char c)
    {
System.out.println(c);
 }
public void disp(char c, intnum)  
{
System.out.println(c + " "+num);
}}
class Sample
{
public static void main(String args[])
   {
DisplayOverloadingobj = new DisplayOverloading( );
obj.disp('a');
obj.disp('a',10);
   }}
